{
  "name": "tables-cms",
  "version": "1.0.0",
  "description": "Gatsby + React CMS for TABLES project",
  "main": "electron-main.js",
  "scripts": {
    "start": "electron .",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "dist": "electron-builder"
  },
  "keywords": [
    "gatsby",
    "react",
    "cms"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "express": "^4.18.2",
    "formidable": "^3.5.4",
    "gatsby": "^5.12.0",
    "gatsby-plugin-express": "^1.1.6",
    "gatsby-source-filesystem": "^5.12.0",
    "intersection-observer": "^0.12.2",
    "mkdirp": "^3.0.1",
    "moment": "^2.30.0",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "fs-extra": "^11.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "electron": "^40.0.0",
    "electron-builder": "^24.13.3"
  },
  "overrides": {
    "react-server-dom-webpack": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "build": {
    "appId": "com.tables.cms",
    "productName": "TABLES CMS",
    "icon": "static/assets/tables-icon.png",
    "files": [
      "main-site/src/**/*",
      "main-site/static/**/*",
      "main-site/.env.development",
      "main-site/gatsby-*.js",
      "main-site/package.json",
      "src/**/*",
      "static/**/*",
      "electron-*.js",
      "gatsby-*.js",
      "gatsby-*.json",
      "package.json",
      "tsconfig.json"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": [
        {
          "target": "mas",
          "arch": [
            "universal"
          ]
        },
        {
          "target": "mas-dev"
        }
      ],
      "artifactName": "tables-cms-macos.${ext}",
      "icon": "static/assets/tables-icon.png"
    },
    "win": {
      "target": "nsis",
      "artifactName": "tables-cms-windows.${ext}",
      "icon": "static/assets/tables-icon.png"
    },
    "linux": {
      "target": "AppImage",
      "artifactName": "tables-cms-linux.${ext}",
      "icon": "static/assets/tables-icon.png"
    }
  }
}
